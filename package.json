{
  "name": "auth-api",
  "version": "0.0.0",
  "main": "main/index.js",
  "repository": "https://github.com/PedroUNB/Auth-Microservice.git",
  "author": "Pedro Oliveira <pedro.oliveira.unb@gmail.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "prepare": "husky install",
    "lint": "eslint ./**/*.ts --fix",
    "test": "jest --passWithNoTests --runInBand --no-cache",
    "test:unit": "npm test -- --watch -c jest-unit-config.js",
    "test:integration": "npm test -- --watch -c jest-integration-config.js",
    "test:staged": "npm test -- --findRelatedTests",
    "test:ci": "npm test -- --coverage",
    "test:coveralls": "npm run test:ci && coveralls < coverage/lcov.info",
    "dev": "ts-node-dev -r tsconfig-paths/register --respawn --transpile-only --watch src/**/*.graphql --ignore-watch node_modules --no-notify --clear src/main"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "@types/amqplib": "^0.10.1",
    "@types/express": "^4.17.16",
    "@types/jest": "^29.2.6",
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "amqplib-mocks": "^1.3.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.0.1",
    "eslint-config-standard-with-typescript": "^32.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "husky": "^8.0.3",
    "jest": "^29.3.1",
    "lint-staged": "^13.1.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "*"
  },
  "engines": {
    "node": "16.x"
  },
  "dependencies": {
    "amqplib": "^0.10.3",
    "express": "^4.18.2"
  }
}
